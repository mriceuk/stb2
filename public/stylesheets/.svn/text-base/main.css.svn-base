body {
	font-family: Helvetica, Arial,  Verdana, sans-serif;
	line-height: 16px;
	color: #FFF;
	background: #FFF;
	margin:0;
	padding:0;
	font-size:12px;
}

#flash
{
	display:block;	
	width:100%;
	height:100%;
	z-index:1;
}

#footer
{
	z-index:6000;
	display:block;
	position:absolute;
	bottom:0px;
	left:0px;
	width:100%;
	height:30px;
	background-color:#1d1103;
}

#footer ul
{
	list-style-type: none;
 	text-align: left;
	padding:8px;
	margin:0px;
	float:right;
}

#footer ul li
{
	margin-right:20px;
	font-size:0.95em;
    list-style: none;
	display: block;
	float: left;
	border:none;
	color:#666;
}


#footer ul li a:link, #footer ul li a:visited
{
	color:#666;
}

